{
  "name": "fp-ts",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "lint": "yarn run eslint . --ext .ts",
    "format": "yarn run prettier . --write && yarn run eslint . --fix --ext .ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'yarn tsc && node dist/main.js'",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "fp-ts": "^2.16.1"
  }
}
